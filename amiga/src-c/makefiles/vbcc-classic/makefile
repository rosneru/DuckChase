# makefile for GNU make (automatically generated by makemake)
# NOTE: indent with TAB in GNU makefiles!

# paths are relative to the project directory (current directory during make)

EXE    = bin/vbcc-classic/DuckChase_c
OBJDIR = o/vbcc-classic/

# compiler and linker

CC = vbcc:bin/vc
LD = vbcc:bin/vc

# target 'all' (default target)

all : $(EXE)

$(OBJDIR)animtools.o : animtools.c
	$(CC) $(shell vbccprefs) -c -o $(OBJDIR)animtools.o animtools.c

$(OBJDIR)imagetools.o : imagetools.c
	$(CC) $(shell vbccprefs) -c -o $(OBJDIR)imagetools.o imagetools.c

$(OBJDIR)main.o : main.c
	$(CC) $(shell vbccprefs) -c -o $(OBJDIR)main.o main.c

$(OBJDIR)timertools.o : timertools.c
	$(CC) $(shell vbccprefs) -c -o $(OBJDIR)timertools.o timertools.c

OBJS = $(OBJDIR)timertools.o $(OBJDIR)main.o $(OBJDIR)imagetools.o $(OBJDIR)animtools.o

$(EXE) : $(OBJS)
	$(LD) $(shell vbccprefs) -o $(EXE) $(OBJS)

# target 'clean'

clean:
	-delete $(EXE)
	-delete $(OBJDIR)\*.o
